{
    "presets": ["es2015", "react"],
    "plugins": [
        "transform-object-rest-spread"
    ],
    "env": {
        "production": {
            "plugins": [
                ["discard-module-references", {
                    "targets": [ "tape" ],
                    "unusedWhitelist": [ "react" ]
                }]
            ]
        }
    }
}
